<% provide(:title, "Text Messages") %>
<% provide(:description, "SMS messages from people.") %>

<h1>Text Messages</h1>
<ol class="breadcrumb">
	<li class="active">Text Messages</li>
	<li><%= link_to 'Statistics', text_message_stats_path %></li>
</ol>

<% @text_messages.each do |text_message| %>
  <%= render 'text_message', :text_message => text_message %>
<% end %>

<%= paginate @text_messages %>
